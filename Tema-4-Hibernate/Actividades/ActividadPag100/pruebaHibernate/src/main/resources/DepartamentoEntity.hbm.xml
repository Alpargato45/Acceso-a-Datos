<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.DepartamentoEntity" table="departamento" schema="public" catalog="empleados">
        <id name="id" column="id"/>
        <property name="nombre" column="nombre"/>
        <property name="presupuesto" column="presupuesto"/>
        <property name="gastos" column="gastos"/>
    </class>
</hibernate-mapping>